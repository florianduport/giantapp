# @ revslider
#: settings/slider_settings_xml_lang.php:12
msgid "Touch Enabled"
msgstr "Dotknij aby aktywować"

# @ revslider
#: settings/slider_settings_xml_lang.php:13
msgid "Enable Swipe Function on touch devices"
msgstr "Uruchom funkcje wstrząsania w urządzeniach dotykowych"

